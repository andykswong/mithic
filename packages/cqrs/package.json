{
  "name": "@mithic/cqrs",
  "version": "0.1.0",
  "description": "CQRS library for mithic",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./preset": "./dist/preset/index.js"
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "prepublishOnly": "npm run clean && npm run build && npm test",
    "clean": "rimraf coverage docs dist",
    "prebuild": "npm run lint",
    "build": "npm run tsc && npm run babel",
    "lint": "eslint src --ext .ts",
    "babel": "babel src -d dist -x '.ts' --out-file-extension .js --root-mode upward",
    "tsc": "tsc --project tsconfig.build.json --emitDeclarationOnly",
    "test": "npm run jest",
    "jest": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest",
    "jest:watch": "npm run jest -- --watch",
    "doc": "typedoc --out docs src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andykswong/mithic.git"
  },
  "keywords": [
    "collaboration",
    "crdt",
    "decentralized",
    "mithic",
    "offline-first",
    "p2p",
    "typescript"
  ],
  "author": "Andy K.S. Wong <andykswong@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andykswong/mithic/issues"
  },
  "homepage": "https://github.com/andykswong/mithic",
  "dependencies": {
    "@mithic/commons": "^0.1",
    "@mithic/messaging": "^0.1",
    "multiformats": "^11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18",
    "@babel/core": "^7.18",
    "@babel/preset-env": "^7.18",
    "@babel/preset-typescript": "^7.18",
    "@types/jest": "^29.2",
    "@types/node": "^18.0",
    "@typescript-eslint/eslint-plugin": "^5.47",
    "@typescript-eslint/parser": "^5.47",
    "babel-jest": "^29.2",
    "cross-env": "^7.0",
    "eslint": "^8.30",
    "jest": "^29.2",
    "rimraf": "^3.0",
    "typedoc": "^0.24",
    "typescript": "^4.9"
  },
  "typedoc": {
    "entryPoint": "src/index.ts"
  }
}
